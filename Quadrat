package com.company;

public class Quadrat {

        private String color;
        private float side;
        public String getColor(){
            return this.color;
        }
        public  void setColor(String color){
            this.color=color;
        }
        public  float getSide(){
            return this.side;
        }
        public  void setSide(float side){
            this.side=side;
        }
        public float calcArea(){
            return this.getSide()*this.getSide();
        }
}

